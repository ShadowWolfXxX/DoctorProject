<AnchorPane id="AnchorPane" prefHeight="423.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Contral.MyBookedAppointmentController">
   <children>
      <VBox alignment="CENTER" prefHeight="423.0" prefWidth="147.0" spacing="20.0" style="-fx-background-color: gray;">
         <children>
            <Label alignment="CENTER" prefHeight="57.0" prefWidth="147.0" text="Patient Appointment" textAlignment="CENTER" wrapText="true">
               <textFill>
                  <LinearGradient endX="0.7350157728706626" endY="1.0">
                     <stops>
                        <Stop>
                           <color>
                              <Color red="0.14386892318725586" green="1.0" blue="0.09000000357627869" />
                           </color>
                        </Stop>
                        <Stop offset="1.0">
                           <color>
                              <Color red="0.8333333134651184" green="1.0" />
                           </color>
                        </Stop>
                     </stops>
                  </LinearGradient>
               </textFill>
               <font>
                  <Font name="Tw Cen MT Condensed Bold" size="24.0" />
               </font>
            </Label>
            <Button fx:id="FreeAppBTN" alignment="CENTER" mnemonicParsing="false" onAction="#FreeAppointment" prefHeight="100.0" prefWidth="149.0" style="-fx-background-color: darkgray;" text="Free Appointment" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <effect>
                  <Lighting diffuseConstant="1.4">
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <Button fx:id="BookedAppBTN" alignment="CENTER" mnemonicParsing="false" onAction="#MyBookedAppointment" prefHeight="102.0" prefWidth="149.0" text="MyBooked Appointment" textAlignment="CENTER" wrapText="true">
               <effect>
                  <Lighting diffuseConstant="1.34">
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <Button fx:id="logoutBTN" mnemonicParsing="false" onAction="#Logout" prefHeight="50.0" prefWidth="113.0" style="-fx-background-radius: 50px; -fx-background-color: red;" text="Log out" textFill="WHITE">
               <font>
                  <Font name="Arial Black" size="12.0" />
               </font>
               <effect>
                  <Lighting diffuseConstant="1.23" specularConstant="1.29" specularExponent="21.53" surfaceScale="3.1">
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant />
                     </light>
                  </Lighting>
               </effect>
            </Button>
         </children>
         <effect>
            <Lighting diffuseConstant="1.31">
               <bumpInput>
                  <Shadow>
                     <input>
                        <DropShadow height="255.0" radius="127.0" width="255.0" />
                     </input>
                  </Shadow>
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
      </VBox>
      <TableView fx:id="viewTable" layoutX="161.0" layoutY="83.0" prefHeight="290.0" prefWidth="465.0">
         <columns>
            <TableColumn fx:id="bookedAppName" prefWidth="137.3333282470703" text="Appointment Name" />
            <TableColumn fx:id="BookedAppstatus" prefWidth="67.33331298828125" text="Status" />
            <TableColumn fx:id="doctorComment" prefWidth="260.6667175292969" text="Doctor Comment" />
         </columns>
      </TableView>
      <Button fx:id="watingAppBTN" layoutX="161.0" layoutY="26.0" mnemonicParsing="false" onAction="#showWating" prefHeight="42.0" prefWidth="211.0" style="-fx-background-color: blue;" text="Show My Wating Appointment" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="finishedAppBTN" layoutX="411.0" layoutY="26.0" mnemonicParsing="false" onAction="#showFinised" prefHeight="42.0" prefWidth="211.0" style="-fx-background-color: green;" text="Show My finished Appointment" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="showCommentBTN" layoutX="161.0" layoutY="383.0" mnemonicParsing="false" onAction="#showComment" prefHeight="18.0" prefWidth="465.0" style="-fx-background-color: lightyellow;" text="Show Doctor Comment...">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow height="7.37" radius="2.6975" spread="0.67" width="5.42" />
         </effect>
      </Button>
   </children>
</AnchorPane>
